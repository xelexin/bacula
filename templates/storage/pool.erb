Pool {
  Name = <%= @pool_name %>
  Pool Type = Backup
  Recycle = <%= @pool_recycle %>
  Auto Prune = <%= @pool_auto_prune %>
  VolumeRetention = <%= @pool_vol_retention %>
  Maximum Volume Bytes = <%= @pool_max_vol_bytes %>
  Storage = <%= @storage_name %>
  Recycle Oldest Volume = <%= @pool_recycle_oldest_vol %>
  Label = "File-"
}
