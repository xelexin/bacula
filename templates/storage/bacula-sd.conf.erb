Storage {
  Name = <%= @storage_name %>
  Working Directory = <%= @storage_work_dir %>
  PID Directory = <%= @storage_pid_dir %>
  SDAddress = <%= @storage_address %>
  SDPort = <%= @storage_port %>
  Maximum Concurrent Jobs = 5
}

Director {
  Name = <%= @director_name %>
  Password = <%= @storage_password %>
}

Messages {
  Name = <%= @storage_name %>
  Director = <%= @director_name %> = all
}
