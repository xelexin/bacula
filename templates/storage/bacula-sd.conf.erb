Storage {
  Name = <%= @storage_name %>
  Working Directory = <%= @storage_work_dir %>
  PID Directory = <%= @storage_pid_dir %>
  SD Address = <%= @storage_address %>
  SD Port = <%= @storage_port %>
}

Director {
  Name = <%= @director_name %>
  Password = <%= @storage_password %>
}

Messages {
  Name = <%= @storage_name %>
  Director = <%= @director_name %> = all
}
